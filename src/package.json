{
  "name": "static",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site",
    "start": "cross-env NODE_ENV=development npm-run-all -l clean --parallel start:* ",
    "start:eleventy": "eleventy --serve --incremental",
    "start:tailwind": "tailwindcss -i ./src/wilderblog.src.css -o ./_site/css/wilderblog.min.css --watch",
    "start:css": "nodemon -e js --exec gulp debug -w ./gulpfile.js -w ./src/wilderblog.src.css",
    "start:js": "rollup ./src/site.js --file ./_site/js/wilderblog.min.js --format iife --sourcemap -w",
    "start:vue": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --watch --mode Development",
    "build": "cross-env NODE_ENV=production npm-run-all -l clean build:*",
    "build:eleventy": "eleventy --quiet",
    "build:tailwind": "tailwindcss -i ./src/wilderblog.src.css -o ./_site/css/wilderblog.min.css -m",
    "build:css": "gulp",
    "build:js": "rollup ./src/site.js --file ./_site/js/wilderblog.min.js --format iife --sourcemap --plugin rollup-plugin-terser",
    "build:vue": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "publish": "run-s -l publish:*",
    "publish:build": "npm run build",
    "publish:container": "pwsh ./publish.ps1",
    "publish:updateIndex": "node uploadIndex.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
